{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "jetbrain-open",
  "title": "JetBrainsOpen",
  "description": "JetBrains全家桶快速打开项目",
  "icon": "extension_icon.png",
  "author": "0reoo",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Open With JetBrains",
      "description": "用JetBrains工具打开项目",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "title": "Shell 脚本位置",
      "name": "shell_path",
      "description": "Shell 脚本位置",
      "type": "directory",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.4",
    "@raycast/utils": "^1.10.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
